@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model form.Models.Employee
<html>
<head>
    <title>Employee Register</title>
</head>
<body>
    <h1>Employee Register</h1>
    <form method="post" asp-action="SubmitForm">
        <div asp-validation-summary="All"></div>
        <label asp-for="EmpId">
            EmpId
        </label>
        <input asp-for="EmpId" />
        <span asp-validation-for="EmpId"></span>
        <br />
        <label asp-for="FistName">
            FistName
        </label>
        <input asp-for="FistName" />
        <span asp-validation-for="FistName"></span>
        <br />
        <label asp-for="LastName">
            LastName
        </label>
        <input asp-for="LastName" />
        <span asp-validation-for="LastName"></span>
        <br />
        <label asp-for="Email">
            Email
        </label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
        <br />
        <label asp-for="Age">
            Age
        </label>
        <input asp-for="Age" />
        <span asp-validation-for="Age"></span>
        <input type="submit" value="submit" />
    </form>
</body>
</html>